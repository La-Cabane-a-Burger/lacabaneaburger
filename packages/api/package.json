{
  "name": "@lacabaneaburger/api",
  "license": "MIT",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "ts-node --transpile-only src/server",
    "build": "prisma generate && tsc --build",
    "generate": "prisma generate",
    "fixtures": "fixtures ./fixtures --require=ts-node/register --require=tsconfig-paths/register",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset -f",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "apollo-server": "^3.5.0",
    "bcryptjs": "2.4.3",
    "graphql": "^16.5.0",
    "jsonwebtoken": "8.5.1",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@getbigger-io/prisma-fixtures-cli": "^1.0.10",
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "^4.14.177",
    "@types/node": "13.11.1",
    "prisma": "^3.13.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=12.16.0 <17.0"
  }
}
